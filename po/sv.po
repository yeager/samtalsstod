msgid "I feel happy right now"
msgstr "Jag känner mig glad nu"

#: src/samtalsstod/main.py:31
msgid "Sad"
msgstr "Ledsen"

#: src/samtalsstod/main.py:31
msgid "I feel sad right now"
msgstr "Jag känner mig ledsen nu"

#: src/samtalsstod/main.py:32
msgid "Thank You"
msgstr "Tack"

#: src/samtalsstod/main.py:32
msgid "Thank you!"
msgstr "Tack!"

